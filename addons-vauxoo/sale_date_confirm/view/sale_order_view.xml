<?xml version='1.0' encoding='UTF-8'?>
<openerp>
    <data>

        <record id="view_inherit_sale_date_confirm" model="ir.ui.view">
            <field name="name">view.inherit.sale.date.confirm</field>
            <field name="model">sale.order</field>
            <field name="inherit_id" ref="sale_order_dates.view_sale_orderfor"/>
            <field name="arch" type="xml">
                <data>
                    <xpath expr="//field[@name='effective_date']" position="after">
                        <field name="date_confirm"/>
                    </xpath>
                </data>
            </field>
        </record>
        
    </data>
</openerp>
