<?xml version="1.0" encoding="utf-8"?>
<openerp>
<data>

	
	<record model="ir.ui.view" id="product_pack_discount_line_form">
		<field name="name">product.pack.discount.line.form</field>
		<field name="model">product.pack.line</field>
		<field name="inherit_id" ref="product_pack.product_pack_line_form"/>
		<field name="type">form</field>
		<field name="arch" type="xml">
			<field name="quantity" position="after">
				<field name="discount"/>
			</field>
		</field>
	</record>

	<record model="ir.ui.view" id="product_pack_discount_line_tree">
		<field name="name">product.pack.discount.line.tree</field>
		<field name="model">product.pack.line</field>
		<field name="type">tree</field>
		<field name="inherit_id" ref="product_pack.product_pack_line_tree"/>
		<field name="arch" type="xml">
			<field name="quantity" position="after">
				<field name="discount"/>
			</field>
		</field>
	</record>
</data>
</openerp>

