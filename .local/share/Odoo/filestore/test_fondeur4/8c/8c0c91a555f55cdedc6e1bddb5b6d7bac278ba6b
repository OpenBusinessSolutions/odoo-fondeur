if(showYAxis){yAxis.scale(y).ticks(yAxis.ticks()&&yAxis.ticks().length?yAxis.ticks():availableHeight/36).tickSize(-availableWidth,0);g.select('.nv-y.nv-axis').call(yAxis);}
